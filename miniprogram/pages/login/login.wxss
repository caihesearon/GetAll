.header {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}
.header image {
    width: 200rpx;
    height: 200rpx; 
}
.content { 
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}
.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
 
.bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}
/* 轮播图样式 */
.slide-image{
  width: 100%;
  height: 300rpx
}
.swiperStyle{
  height: 300rpx
}
/* 图片 */
.img-box{
  width: 80%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}
.icon-image{
  width: 100rpx;
  height: 100rpx;
  margin-left: 15rpx
}
.tex{
  display:block;
}
.box-one{
  height: 160rpx;
}

.Tbtn{
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx;
}

.TbtnW{
  display: flex;
  justify-content: space-around;
  margin-bottom: 40rpx;
}

.BtnBorder{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 130rpx;
  height: 130rpx;
  border:3rpx solid white;
  border-radius: 35rpx;
  background-color: white;
}

.Tbtn image{
  width: 90rpx;
  height: 90rpx;
}